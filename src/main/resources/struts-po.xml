    <package name="po" extends="struts-default">
    	<global-results>
		    <result name="nologin">/login.jsp</result>
		</global-results>
		
    	<action name="pocheckauthen" class="pcpnru.projectAction.PurchaseOrderAction" method="checkauthen">
