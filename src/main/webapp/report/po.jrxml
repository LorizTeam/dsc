d.vendor_id,d.vendor_name,d.vendor_address,d.vendor_road,d.vendor_subdistrict,d.vendor_district,d.vendor_province,d.vendor_postcode,d.vendor_mobile,d.vendor_tel,
d.vendor_tel_ext,d.create_by,d.create_datetime,d.update_by,d.update_datetime,
e.unit_id,e.unit_name,e.create_by,e.create_datetime,e.update_by,e.update_datetime,
z.company_name,z.company_address,z.company_tel,z.company_tel_ext,z.company_mobile,z.company_fax,z.company_vat
FROM
company z, po_hd a 
INNER JOIN po_dt b ON a.po_docno = b.po_docno AND a.`year` = b.`year`
INNER JOIN product c ON b.description = c.product_code
INNER JOIN vendor_master d ON a.vender = d.vendor_id
